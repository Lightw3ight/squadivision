{
	"manifest_version": 2,
	"name": "Squadivision",
	"description": "Share pages with your squad mates, requires a squadvision socket.io server and a squadvision monitor",
	"version": "0.0.1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"activeTab",
		"cookies",
		"http://*/*",
		"https://*/*"
	]
}