{
	"manifest_version": 2,
	"name": "Squadivision Monitor",
	"description": "A monitor extension for squadivision",
	"version": "0.0.0.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"activeTab",
		"cookies",
		"tabs",
		"background",
		"http://*/*",
		"https://*/*"
	]
}